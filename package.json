{
  "name": "website-boilerplate",
  "displayName": "Website Boilerplate",
  "description": "Creates boilerplate files (HTML, CSS, & JS) for a TailwindCSS app",
  "version": "1.0.0",
  "publisher": "jenslys",
  "engines": {
    "vscode": "^1.22.x"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.createTailwindBoilerplate",
    "onCommand:extension.createBoostrapBoilerplate"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.createTailwindBoilerplate",
        "title": "Create Tailwind Boilerplate"
      },
      {
        "command": "extension.createBoostrapBoilerplate",
        "title": "Create Boostrap Boilerplate"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "eslint": "^4.11.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "fs-extra": "^10.0.0"
  }
}
